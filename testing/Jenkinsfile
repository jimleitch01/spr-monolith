pipeline {
    agent {
        label 'maven'
    }
    stages {

        stage('Testing') {
            steps {
                checkout scm
                sh 'echo Testing'
                sh '#find / -name settings-docker.xml '
                sh 'mvn -B -s /usr/share/maven/ref/repository/settings-docker.xml '
            }
        }
    }
}




